kubectl create deploy redis-corretto11 -n spring  --image=emreodabas/hello-app-spring-boot:redis-corretto11
kubectl create deploy redis-zulu11 -n spring  --image=emreodabas/hello-app-spring-boot:redis-zulu11
kubectl create deploy redis-zulu13 -n spring  --image=emreodabas/hello-app-spring-boot:redis-zulu13
kubectl create deploy redis-openjdk11 -n spring  --image=emreodabas/hello-app-spring-boot:redis-openjdk11
kubectl create deploy redis-openjdk16 -n spring  --image=emreodabas/hello-app-spring-boot:redis-openjdk16
kubectl create deploy redis-golang -n golang  --image=emreodabas/hello-app-golang:base
kubectl create deploy redis-nodejs -n nodejs  --image=emreodabas/hello-app-nodejs:base
kubectl create deploy redis-quarkus -n graalvm  --image=emreodabas/hello-app-quarkus:redis-native-11
kubectl create deploy redis-micronaut -n graalvm  --image=emreodabas/hello-app-micronaut:redis-11
kubectl create deploy redis-rust -n rust  --image=emreodabas/hello-app-rust:redis-alpine



kubectl expose deploy redis-corretto11 -n spring --port 8080 --type=NodePort
kubectl expose deploy redis-zulu11 -n spring --port 8080 --type=NodePort
kubectl expose deploy redis-zulu13 -n spring --port 8080 --type=NodePort
kubectl expose deploy redis-openjdk11 -n spring --port 8080 --type=NodePort
kubectl expose deploy redis-openjdk16 -n spring --port 8080 --type=NodePort
kubectl expose deploy redis-golang -n golang --port 8080 --type=NodePort
kubectl expose deploy redis-nodejs -n nodejs --port 8080 --type=NodePort
kubectl expose deploy redis-quarkus -n graalvm --port 8080 --type=NodePort
kubectl expose deploy redis-micronaut -n graalvm --port 8080 --type=NodePort
kubectl expose deploy redis-rust -n rust --port 8080 --type=NodePort



## default a1.medium
kubectl set resources deploy redis-corretto11 -n spring  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-zulu11 -n spring  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-zulu13 -n spring  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-openjdk11 -n spring  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-openjdk16 -n spring  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-golang -n golang  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-nodejs -n nodejs  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-quarkus -n graalvm  --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-micronaut -n graalvm --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"
kubectl set resources deploy redis-rust -n rust --limits=cpu=1,memory="2Gi" --requests=cpu=1,memory="2Gi"




